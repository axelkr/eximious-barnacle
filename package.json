{
  "name": "eximious-barnacle",
  "version": "3.11.0",
  "description": "Frontend webpage for a Kanban board with swimlanes",
  "scripts": {
    "serve": "ng serve",
    "build": "ng build --base-href ./",
    "buildProduction": "ng build --base-href ./ --configuration production",
    "test": "ng test --browsers=Firefox",
    "testCLI": "ng test --no-watch --no-progress --browsers=FirefoxHeadlessCI --reporters=junit",
    "lint": "ng lint",
    "bundle-report": "ng build --base-href ./ --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "repository": {
    "type": "git",

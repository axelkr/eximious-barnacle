    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/d3": "^7.0.0",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^14.14.39",
    "@typescript-eslint/eslint-plugin": "4.29.0",
